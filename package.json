{
  "name": "vuex-no-map",
  "version": "1.0.11",
  "description": "Vuex plugin, which allows to use store functionality without need to use vuex mappers in components",
  "main": "dist/index.js",
  "scripts": {
    "compile": "babel src/index.js --out-dir dist",
    "uglify": "uglifyjs dist/index.js -c -m -o dist/index.js",
    "prepublish": "npm run compile && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p1pchenk0/vuex-no-map.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "state",
    "state management",
    "mapstate",
    "mapgetters",
    "mapactions",
    "mapmutations",
    "map"
  ],
  "author": "Artur Pipchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/p1pchenk0/vuex-no-map/issues"
  },
  "homepage": "https://github.com/p1pchenk0/vuex-no-map#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "uglify-js": "^3.7.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3"
  }
}
